{
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 120,
	"arrowParens": "always",
	"endOfLine": "lf",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"singleAttributePerLine": true,
	"vueIndentScriptAndStyle": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true
}
